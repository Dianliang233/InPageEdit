{
  "name": "mediawiki-inpageedit",
  "version": "15.0.0-alpha.0",
  "description": "A useful MediaWiki JavaScript Plugin written with jQuery",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@dragon-fish/bump": "^0.0.13",
    "@types/jquery": "^3.5.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.7.0",
    "file-loader": "^6.2.0",
    "prettier": "^2.1.2",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.18",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "build": "yarn clear && webpack && MINIFY=1 webpack",
    "bump": "bump",
    "clear": "rm -rf ./dist ./**/dist ./dev",
    "dev": "yarn clear && webpack --watch -o ./dev",
    "lint": "eslint ./index.js ./module/*.js ./method/*.js",
    "prettier": "prettier --write ./index.js ./module/*.js ./method/*.js",
    "publish:stable": "npm run build && npm publish --tag latest",
    "publish:canary": "npm run build && npm publish --tag canary && yarn clear",
    "style": "stylus ./style/index.styl -o ./style/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wjghj-Project/InPageEdit.git"
  },
  "keywords": [
    "mediawiki",
    "mediawiki-gadget",
    "inpageedit"
  ],
  "author": "Dragon-Fish",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Wjghj-Project/InPageEdit/issues"
  },
  "homepage": "https://ipe.js.org"
}
